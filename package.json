{
  "name": "socket_sync",
  "version": "0.0.1",
  "author": "Dean Netherton",
  "license": "MIT",
  "description": "A linux domain socket library to publish messages",
  "main": "index.js",
  "engines": {
    "node": "12.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vipoo/socket_sync.git"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "scripts"
      ],
      "extensions": "js",
      "quiet": true,
      "legacyWatch": false,
      "delay": 1000,
      "runOnChangeOnly": false,
      "inherit": true
    },
    "test": {
      "patterns": [
        "src",
        "tests"
      ],
      "extensions": "js",
      "quiet": true,
      "legacyWatch": false,
      "delay": 1000,
      "runOnChangeOnly": false,
      "inherit": true
    }
  },
  "scripts": {
    "generate:docs": "jsdoc -r --readme README.md -c .jsdoc.json -d ./docs ./src",
    "build": "./scripts/build.sh",
    "lint": "eslint --fix src tests",
    "clean": "./scripts/clean.sh",
    "pretest": "./scripts/clean.sh",
    "test": "NODE_ENV=test mocha --require \"@babel/register\" tests/**",
    "tst": "NODE_ENV=test mocha --require \"@babel/register\" tests/**",
    "posttest": "eslint --fix src tests",
    "watch": "npm-watch",
    "prepack": "./scripts/clean.sh && ./scripts/build.sh"
  },
  "dependencies": {
    "async_iter": "^1.1.3",
    "core-js": "^3.1.4",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-partial-application": "^7.4.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^11.0.0-beta.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "docdash": "^1.1.1",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-stickler": "^1.0.6",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-no-only-tests": "^2.3.1",
    "eslint-plugin-node": "^9.1.0",
    "jsdoc": "^3.6.3",
    "mocha": "^10.2.0",
    "npm-watch": "^0.6.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
}
